month = {sep},
number = {9},
pages = {1061--7},
pmid = {16925655},
title = {{Ecosystem productivity can be predicted from potential relative growth rate and species abundance.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16925655},
volume = {9},
year = {2006}
}
