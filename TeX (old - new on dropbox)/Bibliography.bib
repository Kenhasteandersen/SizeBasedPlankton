keywords = {Baltic Sea,historical data,population dynamics,sprat (Sprattus sprattus balticus)},
number = {6},
pages = {1010--1018},
title = {{Reconstructing the population dynamics of sprat (Sprattus sprattus balticus) in the Baltic Sea in the 20th century}},
volume = {69},
year = {2012}
}
