@article{Ravn-Jonsen2016,
abstract = {Ecosystem externalities arise when one use of an ecosystem affects its$\backslash$nother uses through the production functions of the ecosystem. We use$\backslash$nsimulations with a size-spectrum ecosystem model to investigate the$\backslash$necosystem externality created by fishing of multiple species. The model$\backslash$nis based upon general ecological principles and is calibrated to the$\backslash$nNorth Sea. Two fleets are considered: a forage fish fleet targeting$\backslash$nspecies that mature at small sizes and a large fish fleet targeting$\backslash$nlarge piscivorous species. Based on the marginal analysis of the present$\backslash$nvalue of the rent, we develop a benefit indicator that explicitly$\backslash$ndivides the consequences of fishing into internal and external benefits.$\backslash$nThis analysis demonstrates that the forage fish fleet has a notable$\backslash$neconomic impact on the large fish fleet, but the reverse is not true.$\backslash$nThe impact can be either negative or positive, which entails that for$\backslash$noptimal economic exploitation, the forage fishery has to be adjusted$\backslash$naccording to the large fish fishery. With the present large fish fishery$\backslash$nin the North Sea, the two fisheries are well adjusted; however, the$\backslash$npresent combined exploitation level is too high to achieve optimal$\backslash$neconomic rents.},
author = {Ravn-Jonsen, L and Andersen, K H. and Vestergaard, N},
doi = {10.1111/nrm.12094},
