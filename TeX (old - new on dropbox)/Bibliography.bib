title = {{Size-selective fishing drives species composition in the Celtic Sea}},
volume = {69},
year = {2012}
}
@article{Fung2013,
abstract = {A dynamic food-web model of more than 1000 species was used to quantify the recovery trajectory of marine community size-structure under different hypothetical fishing regimes, using the Northeast Atlantic as an example. Size-structure was summarised by 4 indica- tors: the Large Fish Indicator (LFI), the Large Species Indicator (LSI), the biomass-weighted mean maximum length of fish species (L – max) and the biomass-weighted mean maturation length of fish species (L - mat). Time-series of these indicators recorded recovery following release from fishing with various size-selectivities, intensities and durations. In model simulations, fishing-induced trophic cascades were observed to distort fish community size-structure, but these did not have a large influence on recovery level or duration as measured by the 4 indicators. However, simula- tions showed that local extinctions of large fish species increased in number with both fishing intensity and duration, and could strongly limit the recovery level. Recovery of fish community size-structure to near equilibrium frequently took multiple decades in simulations; these long transient periods suggest that management interventions for size-structure recovery may require much longer than previously thought. Our results demonstrate the need for community-level modelling to set realistic targets for management of community size-structure},
author = {Fung, Tak and Farnsworth, Keith D. and Shephard, Samuel and Reid, David G. and Rossberg, Axel G.},
doi = {10.3354/meps10305},
issn = {01718630},
journal = {Marine Ecology Progress Series},
keywords = {Dynamic model,Ecosystem approach to fisheries management,Extinction,Fisheries indicators,Food-web,Resilience,Species richness},
pages = {155--171},
title = {{Why the size structure of marine communities can require decades to recover from fishing}},
volume = {484},
year = {2013}
}
