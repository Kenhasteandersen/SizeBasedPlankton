pmid = {20212124},
title = {{Evolutionary games in the multiverse.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2851774{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {107},
