pmid = {20538646},
title = {{Direct and indirect climate forcing in a multi-species marine system.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2982221{\&}tool=pmcentrez{\&}rendertype=abstract},
