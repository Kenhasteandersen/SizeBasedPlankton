month = {jan},
number = {5},
pages = {e5529},
pmid = {19479037},
title = {{Intense habitat-specific fisheries-induced selection at the molecular Pan I locus predicts imminent collapse of a major cod fishery.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2682699{\&}tool=pmcentrez{\&}rendertype=abstract},
