doi = {10.1006/jtbi.2001.2370},
issn = {0022-5193},
journal = {Journal Theoretical Biology},
keywords = {Animals,Biological,Computer Simulation,Ecology,Food Chain,Models},
month = {sep},
number = {2},
pages = {223--235},
pmid = {11531387},
title = {{Global stability, local stability and permanence in model food webs}},
