@article{Gardmark2006,
abstract = {Body size is an important determinant of resource use, fecundity and mortality risk. Evolution of maturation size in response to size-dependent selection is thus a fundamental part of life-history theory. Increased mortality among small individuals has previously been predicted to cause larger maturation size, whereas increased mortality among large individuals is expected to have the opposite effect. Here we use a continuously size-structured model to demonstrate that, contrary to these widespread expectations, increased mortality among small individuals can have three alternative effects: maturation size may increase, decrease or become evolutionarily bistable. We show that such complex responses must be reckoned with whenever mortality is size-dependent, growth is indeterminate, reproduction impairs growth and fecundity increases with size. Predicting adaptive responses to altered size-dependent mortality is thus inherently difficult, since, as demonstrated here, such mortality cannot only reverse the direction of adaptation, but also cause abrupt shifts in evolutionarily stable maturation sizes.},
author = {G{\aa}rdmark, Anna and Dieckmann, Ulf},
doi = {10.1098/rspb.2006.3562},
issn = {0962-8452},
journal = {Proceedings. Biological sciences / The Royal Society},
keywords = {Adaptation, Biological,Adaptation, Biological: physiology,Animals,Biological Evolution,Body Size,Computer Simulation,Models, Biological,Mortality,Selection, Genetic,Sexual Maturation,Sexual Maturation: physiology},
month = {oct},
number = {1598},
pages = {2185--92},
pmid = {16901838},
title = {{Disparate maturation adaptations to size-dependent mortality.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1635525{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {273},
year = {2006}
}
