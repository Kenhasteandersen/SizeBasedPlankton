title = {{Self-organized instability in complex ecosystems.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1692980{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {357},
year = {2002}
}
